#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../yaml-diff-patch/dist/bin/yaml-patch.js" "$@"
else 
  exec node  "$basedir/../yaml-diff-patch/dist/bin/yaml-patch.js" "$@"
fi
